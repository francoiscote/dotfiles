{
  "breadcrumbs.enabled": true,
  "diffEditor.renderSideBySide": false,
  "editor.scrollBeyondLastLine": false,
  "editor.tabSize": 2,
  "explorer.confirmDelete": false,
  "git.confirmSync": false,
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.cursorBlinking": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.iconTheme": "material-icon-theme",
  "emmet.triggerExpansionOnTab": true,
  "php.validate.executablePath": "/usr/bin/php",
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.folders.color": "#3e8fb0",
  "workbench.editor.closeEmptyGroups": false,
  "editor.minimap.enabled": true,
  "editor.minimap.maxColumn": 200,
  "editor.minimap.showSlider": "always",
  "editor.minimap.renderCharacters": false,
  "editor.smoothScrolling": true,
  "editor.cursorBlinking": "phase",
  "workbench.editor.focusRecentEditorAfterClose": false,
  "editor.renderControlCharacters": false,
  "files.associations": {
    "Dockerfile.*": "dockerfile"
  },
  "git.autofetch": true,
  "editor.fontFamily": "OperatorMonoSSmLig Nerd Font",
  "editor.fontLigatures": true,
  "editor.letterSpacing": -0.3,
  "color-highlight.markRuler": false,
  "color-highlight.markerType": "dot-before",
  "zenMode.fullScreen": false,
  "window.title": "${rootName}",
  "git.enableSmartCommit": true,
  "material-icon-theme.folders.theme": "specific",
  "workbench.preferredDarkColorTheme": "Community Material Theme Palenight High Contrast",
  "workbench.preferredLightColorTheme": "GitHub Light Default",
  "terminal.integrated.macOptionClickForcesSelection": true,
  "explorer.confirmDragAndDrop": false,
  "todo-tree.filtering.useBuiltInExcludes": "file and search excludes",
  "todo-tree.general.statusBar": "total",
  "todo-tree.highlights.defaultHighlight": {
    "foreground": "#292D3E",
    "background": "#FFCB6B",
    "iconColour": "#FFCB6B"
  },
  "typescript.disableAutomaticTypeAcquisition": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.renderWhitespace": "trailing",
  "extensions.ignoreRecommendations": false,
  "search.useGlobalIgnoreFiles": true,
  "search.exclude": {
    "**/.next": true,
    "**/.nyc_output": true,
    "**/coverage": true
  },
  "material-icon-theme.saturation": 1,
  "terminal.integrated.fontWeightBold": "500",
  "editor.fontWeight": 400,
  "diffEditor.ignoreTrimWhitespace": false,
  "vetur.validation.template": false,
  "vetur.validation.templateProps": true,
  "vetur.validation.interpolation": false,
  "workbench.colorTheme": "Ros√© Pine Moon",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#f6c177",
    "editor.selectionBackground": "#f6c17733"
  },
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.fontFamily": "OperatorMonoSSmLig Nerd Font",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "fontshortcuts.defaultFontSize": 15,
  "editor.lineHeight": 0,
  "editor.fontSize": 16
}
