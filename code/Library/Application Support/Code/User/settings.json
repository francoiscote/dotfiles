{
  "editor.fontSize": 14,
  "editor.fontFamily": "Operator Mono SSm Lig, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.renderWhitespace": "all",
  "workbench.colorTheme": "One Dark Pro Italic",
  "workbench.iconTheme": "vscode-great-icons",
  "workbench.activityBar.visible": true,
  "git.confirmSync": false,
  "explorer.confirmDelete": false,
  "explorer.autoReveal": false,
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontSize": 14,
  "workbench.statusBar.feedback.visible": false,
  "workbench.colorCustomizations": {
    "terminal.ansiRed": "#e06c75",
    "terminal.ansiBrightRed": "#e06c75",
    "terminal.ansiGreen": "#98c379",
    "terminal.ansiBrightGreen": "#98c379",
    "terminal.ansiYellow": "#e5c07b",
    "terminal.ansiBrightYellow": "#e5c07b",
    "terminal.ansiBlue": "#56b6c2",
    "terminal.ansiBrightBlue": "#56b6c2",
    "terminal.ansiMagenta": "#c678dd",
    "terminal.ansiBrightMagenta": "#c678dd",
    "terminal.ansiCyan": "#61afef",
    "terminal.ansiBrightCyan": "#61afef"
  },
  "diffEditor.renderSideBySide": false,
  "editor.scrollBeyondLastLine": false,
  "editor.tabSize": 2,
  "editor.lineHeight": 0,
  "prettier.eslintIntegration": true,
  "editor.formatOnSave": false,
  "breadcrumbs.enabled": true,
  "editor.formatOnPaste": false,
  "editor.formatOnType": false,
  "window.zoomLevel": 0,
  "search.location": "panel",
  "typescript.updateImportsOnFileMove.enabled": "always"
}
